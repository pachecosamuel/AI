{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI (Poetry)",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "main:app",
        "--reload",
        "--port",
        "8080"
      ],
      "cwd": "${workspaceFolder}/src",
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ]
}
